    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prepare": "husky install",
    "prisma:seed": "prisma db seed --preview-feature --schema=./apps/api-starter/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./apps/api-starter/prisma/schema.prisma",
    "prisma:gen": "prisma generate --schema=./apps/api-starter/prisma/schema.prisma",
    "tailwind:gen": "tailwindcss -i ./apps/next-starter/styles/tailwind.css -o ./apps/next-starter/styles/tailwind.generated.css --jit",
    "tailwind:watch": "tailwindcss -i ./apps/next-starter/styles/tailwind.css -o ./apps/next-starter/styles/tailwind.generated.css --watch --jit"
  },
