  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "dev": "yarn api & yarn client",
    "marketing": "nx serve marketing-starter --port=4300",
    "api": "yarn prisma:gen && nx serve api-starter",
    "client": "yarn tailwind:gen && nx serve next-starter",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
